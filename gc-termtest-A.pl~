open(termtest1,">./gc-termtest-A1.tex");
open(termtest2,">./gc-termtest-A2.tex");
print termtest1 "% gc-termtest-A.tex\n";
print termtest2 "% gc-termtest-A.tex\n";

print termtest1 "\\documentclass[11pt]{article}\n";
print termtest2 "\\documentclass[11pt]{article}\n";
print termtest1 "\\usepackage{alltt}\n";
print termtest2 "\\usepackage{alltt}\n";
print termtest1 "\\usepackage{enumerate}\n";
print termtest2 "\\usepackage{enumerate}\n";
print termtest1 "\\usepackage{syllogism} \n";
print termtest2 "\\usepackage{syllogism} \n";
print termtest1 "\\usepackage{october}\n";
print termtest2 "\\usepackage{october}\n";
print termtest1 "\\usepackage[table]{xcolor}\n";
print termtest2 "\\usepackage[table]{xcolor}\n";
print termtest1 "\\pagestyle{empty}\n";
print termtest2 "\\pagestyle{empty}\n";

$nm[0]="termtest1";
$nm[1]="termtest2";
$vn[0]="1";
$vn[1]="2";

$v001[0]="1";
$v002[0]="\\lim_{x\\rightarrow{}5}\\left(\\frac{1}{x-5}-\\frac{10}{x^{2}-25}\\right)";
$v003[0]="y=\\frac{1}{5\\sin{}x+2\\cos{}x}";
$v004[0]="0.5";
$v005[0]="3";
$v006[0]="g(t)=-7e^{t\\cos{}t}";
$v007[0]="Polonium-210 The half-life of polonium is 139 days, but your sample will not be useful to you after 95\\% of the radioactive nuclei present on the day the sample arrives has disintegrated. For about how many days after the sample arrives will you be able to use the polonium?";
$v008[0]="65";
$v009[0]="35";
$v010[0]="50";

$v001[1]="2";
$v002[1]="\\lim_{x\\rightarrow{}4}\\left(\\frac{1}{x-4}-\\frac{8}{x^{2}-16}\\right)";
$v003[1]="y=\\frac{1}{5\\cos{}x+2\\sin{}x}";
$v004[1]="\\frac{1}{2}";
$v005[1]="2";
$v006[1]="g(t)=-7e^{t\\sin{}t}";
$v007[1]="The half-life of Palladium-100 is 4 days. After 20 days a sample of Palladium-100 has been reduced to a mass of 1 mg. What was the initial mass (in mg) of the sample?";
$v008[1]="70";
$v009[1]="30";
$v010[1]="45";

print termtest1 "\\newcounter{aufg}\n";
print termtest2 "\\newcounter{aufg}\n";
print termtest1 "\\setcounter{aufg}{0}\n";
print termtest2 "\\setcounter{aufg}{0}\n";
print termtest1 "\\newcommand{\\aufgabe}[1]{\\refstepcounter{aufg}\\textbf{(\\arabic{aufg})}[#1 points]}\n";
print termtest2 "\\newcommand{\\aufgabe}[1]{\\refstepcounter{aufg}\\textbf{(\\arabic{aufg})}[#1 points]}\n";

print termtest1 "\\begin{document}\n";
print termtest2 "\\begin{document}\n";

for ($i1=0;$i1<=$#v001;++$i1)
{
    print $nm[$i1] "\\textbf{Term Test A version $vn[$i1]}\n";

    print $nm[$i1] "\\aufgabe{5} Evaluate the limit\n";
    print $nm[$i1] "\\begin{equation}\n";
    print $nm[$i1] "  \\label{eq:wohghoda}\n";
    print $nm[$i1] "  {\\ufoj}\\notag\n";
    print $nm[$i1] "\\end{equation}\n";

    print $nm[$i1] "\\aufgabe{8} Find the equation of the tangent line to the curve\n";
    print $nm[$i1] "\\begin{equation}\n";
    print $nm[$i1] "  \\label{eq:leegaigh}\n";
    print $nm[$i1] "  {\\mair}\\notag\n";
    print $nm[$i1] "\\end{equation}\n";
    print $nm[$i1] "at the point $(0,{\\utit})$. \n";

    print $nm[$i1] "\\aufgabe{7} Find the domain and the derivative of\n";
    print $nm[$i1] "\\begin{equation}\n";
    print $nm[$i1] "  \\label{eq:javahvap}\n";
    print $nm[$i1] "  f(x)=\\frac{\\ln{}x}{{\\jief}+\\ln{}x}\\notag\n";
    print $nm[$i1] "\\end{equation}\n";
    print $nm[$i1] "Make sure to simplify the derivative as much as possible.\n";

    print $nm[$i1] "\\aufgabe{5} Find the derivative of \n";
    print $nm[$i1] "\\begin{equation}\n";
    print $nm[$i1] "  \\label{eq:ohjoceej}\n";
    print $nm[$i1] "  {\\caib}\\notag\n";
    print $nm[$i1] "\\end{equation}\n";

    print $nm[$i1] "\\aufgabe{7} {\\eizi}\n";

    print $nm[$i1] "\\aufgabe{8} Provide the second derivative of the following two\n";
    print $nm[$i1] "functions in their simplest form. \n";
    print $nm[$i1] "\\begin{equation}\n";
    print $nm[$i1] "  \\label{eq:loesheop}\n";
    print $nm[$i1] "  f(x)=5x-\\frac{3}{x-2}\\notag\n";
    print $nm[$i1] "\\end{equation}\n";
    print $nm[$i1] "\\begin{equation}\n";
    print $nm[$i1] "  \\label{eq:eighohbi}\n";
    print $nm[$i1] "  g(x)=\\frac{x^{2}\\ln{}x}{2}-\\frac{3x^{2}}{4}\\notag\n";
    print $nm[$i1] "\\end{equation}\n";

    print $nm[$i1] "\\aufgabe{9} ";
    print $nm[$i1] "An aluminum beam was brought from the outside cold into a machine shop where the temperature was held at {\\afie}$^{\\circ}$F. After 10 minutes, the beam warmed to {\\uphe}$^{\\circ}$F and after another 10 minutes it was {\\kieg}$^{\\circ}$F. Use Newton's law of cooling to estimate the beam's initial temperature.\n";
}

print termtest1 "\\end{document}\n";
print termtest2 "\\end{document}\n";

close(termtest2);
close(termtest1);
