\begin{document}

\textbf{Term Test C Preparation}

{\prepprob} Find the area enclosed by the curves
\begin{equation}
  \label{eq:shohgaje}
  x=y^{2}-y\mbox{ and }x=y-y^{2}\notag
\end{equation}
% Protter and Protter, page 227, exercise 32

{\prepprob} $S$ is a solid generated by revolving a bounded region $R$
about the $x$-axis. Find the volume of $S$. $R$ is bounded by $x+y=5$
and $xy=4$.
% Protter and Protter, page 232, exercise 20

{\prepprob} Find the length of the following arc,
\begin{equation}
  \label{eq:chohquai}
  x=\frac{1}{3}(y-3)\sqrt{y},0\leq{}y\leq{}3\notag
\end{equation}
% Protter and Protter, page 241, exercise 7

{\prepprob} Find the area of the lateral surface $S$ obtained by
revolving the given arc $C$ around the $x$-axis. 
\begin{equation}
  \label{eq:ohyihooy}
  C=\{(x,y):0\leq{}x\leq{}2,y^{2}=2-x\}\notag
\end{equation}
Note that
\begin{equation}
  \label{eq:eidojapa}
\int\sqrt{ax+b}\,dx=\frac{2}{3a}\cdot\left(ax+b\right)^{\frac{3}{2}}+C
\end{equation}
% Protter and Protter, page 246, exercise 5

{\prepprob} Find the indefinite integral
\begin{equation}
  \label{eq:aarahghe}
  \int\frac{e^{-2x}}{\sqrt{1-e^{-4x}}}\,dx\notag
\end{equation}
Note that
\begin{equation}
  \label{eq:ahficani}
  \int\frac{1}{\sqrt{1-x^{2}}}\,dx=\arcsin{}x+C
\end{equation}

% Protter and Protter, page 282, exercise 14

{\prepprob} Find the following indefinite integrals. Remember that the
derivative of $f(x)=\tan{}x$ is $f'(x)=\sec^{2}x$.
\begin{equation}
  \label{eq:jeepaesu}
  \int\frac{3x-2}{4x+7}\,dx\notag
\end{equation}
% Protter and Protter, page 298, exercise 70
\begin{equation}
  \label{eq:niquohro}
  \int{}e^{\tan{}x}\sec^{2}x\,dx\notag
\end{equation}
% Protter and Protter, page 298, exercise 73

{\prepprob} Evaluate

\begin{equation}
  \label{eq:aegashee}
  \int_{0}^{\frac{\pi}{4}}\sin^{2}2x\cos^{2}2x\,dx\notag
\end{equation}
\begin{equation}
  \label{eq:xeethieb}
  \int_{0}^{\frac{\pi}{4}}\cos^{2}3x\sin^{3}3x\,dx\notag
\end{equation}
% Protter and Protter, page 308, exercise 8 and 12

{\prepprob} Use integration by parts to integrate
\begin{equation}
  \label{eq:zaameihe}
  \int{}x^{3}e^{2x}\,dx\notag
\end{equation}
\begin{equation}
  \label{eq:bahvogow}
  \int_{0}^{\frac{\pi}{4}}e^{3x}\sin{}4x\,dx\notag
\end{equation}
% Protter and Protter, page 304, exercise 6 and 39

\end{document}

